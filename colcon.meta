names:
  # Task manager cannot use lld, for reasons I don't understand
  task_manager:
    cmake-args:
      - "-DCMAKE_C_COMPILER=clang"
      - "-DCMAKE_CXX_COMPILER=clang++"
      - "-DCMAKE_CXX_FLAGS='-fopenmp=libomp'"
      - "-DCMAKE_C_FLAGS='-fopenmp=libomp'"
      - "-DCMAKE_EXE_LINKER_FLAGS=''"
      - "-DCMAKE_MODULE_LINKER_FLAGS=''"
      - "-DCMAKE_SHARED_LINKER_FLAGS=''"

  # These must be compiled with GCC, for reasons I can't be bothered to determine right now
  rs_to_velodyne:
    cmake-args:
      - "-DCMAKE_C_COMPILER=/usr/bin/gcc"
      - "-DCMAKE_CXX_COMPILER=/usr/bin/g++"
      - "-DCMAKE_CXX_FLAGS=''"
      - "-DCMAKE_C_FLAGS=''"
      - "-DCMAKE_EXE_LINKER_FLAGS=''"
      - "-DCMAKE_MODULE_LINKER_FLAGS=''"
      - "-DCMAKE_SHARED_LINKER_FLAGS=''"
  mavros:
